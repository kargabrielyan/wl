<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#fefce8"/>
  
  <!-- Коробка с красками -->
  <rect x="150" y="80" width="100" height="60" fill="#fbbf24" stroke="#f59e0b" stroke-width="2"/>
  <rect x="160" y="90" width="80" height="40" fill="#ffffff"/>
  
  <!-- Краски в коробке -->
  <circle cx="170" cy="110" r="8" fill="#ef4444"/>
  <circle cx="190" cy="110" r="8" fill="#3b82f6"/>
  <circle cx="210" cy="110" r="8" fill="#10b981"/>
  <circle cx="230" cy="110" r="8" fill="#f59e0b"/>
  
  <!-- Кисти -->
  <rect x="80" y="100" width="4" height="40" fill="#8b5cf6"/>
  <rect x="82" y="95" width="2" height="10" fill="#d97706"/>
  
  <rect x="90" y="105" width="4" height="35" fill="#8b5cf6"/>
  <rect x="92" y="100" width="2" height="10" fill="#d97706"/>
  
  <rect x="100" y="110" width="4" height="30" fill="#8b5cf6"/>
  <rect x="102" y="105" width="2" height="10" fill="#d97706"/>
  
  <!-- Альбом -->
  <rect x="280" y="90" width="60" height="80" fill="#ffffff" stroke="#6b7280" stroke-width="2"/>
  <rect x="285" y="95" width="50" height="70" fill="#f3f4f6"/>
  
  <!-- Рисунок в альбоме -->
  <circle cx="310" cy="120" r="15" fill="#fbbf24"/>
  <path d="M 295 140 Q 310 150 325 140" stroke="#f59e0b" stroke-width="2" fill="none"/>
  <circle cx="305" cy="115" r="2" fill="#1f2937"/>
  <circle cx="315" cy="115" r="2" fill="#1f2937"/>
  
  <!-- Карандаши -->
  <rect x="70" y="180" width="6" height="40" fill="#ef4444" transform="rotate(-15 73 200)"/>
  <rect x="85" y="185" width="6" height="40" fill="#3b82f6" transform="rotate(-10 88 205)"/>
  <rect x="100" y="190" width="6" height="40" fill="#10b981" transform="rotate(-5 103 210)"/>
  
  <!-- Ластик -->
  <rect x="320" y="180" width="20" height="12" fill="#fbbf24" rx="2"/>
  
  <!-- Декоративные элементы -->
  <circle cx="50" cy="50" r="20" fill="#ec4899" opacity="0.3"/>
  <circle cx="350" cy="60" r="15" fill="#06b6d4" opacity="0.3"/>
  <circle cx="60" cy="250" r="12" fill="#10b981" opacity="0.3"/>
  
  <text x="200" y="260" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="#d97706">Набор для рисования</text>
</svg>
