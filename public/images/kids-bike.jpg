<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f0fdf4"/>
  
  <!-- Рама велосипеда -->
  <path d="M 100 200 L 150 150 L 200 120 L 250 150 L 300 200" stroke="#ef4444" stroke-width="8" fill="none"/>
  
  <!-- Колеса -->
  <circle cx="120" cy="200" r="30" fill="#ffffff" stroke="#374151" stroke-width="3"/>
  <circle cx="120" cy="200" r="20" fill="#f3f4f6" stroke="#6b7280" stroke-width="2"/>
  <circle cx="120" cy="200" r="5" fill="#374151"/>
  
  <circle cx="280" cy="200" r="30" fill="#ffffff" stroke="#374151" stroke-width="3"/>
  <circle cx="280" cy="200" r="20" fill="#f3f4f6" stroke="#6b7280" stroke-width="2"/>
  <circle cx="280" cy="200" r="5" fill="#374151"/>
  
  <!-- Спицы колес -->
  <line x1="120" y1="200" x2="100" y2="180" stroke="#6b7280" stroke-width="1"/>
  <line x1="120" y1="200" x2="140" y2="180" stroke="#6b7280" stroke-width="1"/>
  <line x1="120" y1="200" x2="100" y2="220" stroke="#6b7280" stroke-width="1"/>
  <line x1="120" y1="200" x2="140" y2="220" stroke="#6b7280" stroke-width="1"/>
  
  <line x1="280" y1="200" x2="260" y2="180" stroke="#6b7280" stroke-width="1"/>
  <line x1="280" y1="200" x2="300" y2="180" stroke="#6b7280" stroke-width="1"/>
  <line x1="280" y1="200" x2="260" y2="220" stroke="#6b7280" stroke-width="1"/>
  <line x1="280" y1="200" x2="300" y2="220" stroke="#6b7280" stroke-width="1"/>
  
  <!-- Руль -->
  <path d="M 200 120 L 200 100 L 180 90 L 220 90 L 200 100" stroke="#ef4444" stroke-width="6" fill="none"/>
  
  <!-- Седло -->
  <ellipse cx="200" cy="120" rx="15" ry="8" fill="#8b5cf6"/>
  
  <!-- Педали -->
  <circle cx="150" cy="200" r="8" fill="#374151"/>
  <circle cx="250" cy="200" r="8" fill="#374151"/>
  
  <!-- Дополнительные колеса -->
  <circle cx="100" cy="230" r="15" fill="#fbbf24" opacity="0.7"/>
  <circle cx="300" cy="230" r="15" fill="#fbbf24" opacity="0.7"/>
  
  <text x="200" y="280" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="#dc2626">Детский велосипед</text>
</svg>
